tends EventEmitter {\n  /**\n   * Contructor de la clase que emite un evento de tipo request cuando el mensaje le llega completo\n   * @param connection Socket\n   */\n  constructor(connection: EventEmitter) {\n    super();\n\n    let wholeData = '';\n    connection.on('data', (dataChunk) => {\n      wholeData += dataChunk;\n\n      let messageLimit = wholeData.indexOf('\\n');\n      while (messageLimit !== -1) {\n        const message = wholeData.substring(0, messageLimit);\n        wholeData = wholeData.substring(messageLimit + 1);\n        this.emit('request', JSON.parse(message));\n        messageLimit = wholeData.indexOf('\\n');\n      }\n    });\n  }\n}\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"1bd37d29caaa26462723dec67ee298beaf6e5a9c","contentHash":"9af60f89f828db42afdf4ba549c57f913f531fefc608a9645f58488d1d6879a2"}}